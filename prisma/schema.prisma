generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Merchant {
  id         String   @id
  name       String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  deleted_at DateTime?

  @@map("merchant")
}
